<gconfschemafile>
  <schemalist>
    
    <schema>
       <key>/schemas/apps/twitux/auth/user_id</key>
       <applyto>/apps/twitux/auth/user_id</applyto>
       <owner>twitux</owner>
       <type>string</type>
       <default></default>
       <locale name="C">
          <short>Twitter username</short>
          <long>User's Twitter account username.</long>
       </locale>
    </schema>

    <schema>
       <key>/schemas/apps/twitux/auth/password</key>
       <applyto>/apps/twitux/auth/password</applyto>
       <owner>twitux</owner>
       <type>string</type>
       <default></default>
       <locale name="C">
          <short>Twitter password</short>
          <long>User's Twitter account password.</long>
       </locale>
    </schema>
    
    <schema>
    	<key>/schemas/apps/twitux/auth/auto_login</key>
    	<applyto>/apps/twitux/auth/auto_login</applyto>
    	<owner>twitux</owner>
    	<type>bool</type>
    	<default>true</default>
    	<locale name="C">
    	   <short>Log automatically into Twitter</short>
    	   <long>Log automatically into Twitter.</long>
    	</locale>
    </schema>
    
    <schema>
    	<key>/schemas/apps/twitux/tweets/home_timeline</key>
    	<applyto>/apps/twitux/tweets/home_timeline</applyto>
    	<owner>twitux</owner>
    	<type>string</type>
    	<default>http://twitter.com/statuses/friends_timeline.xml</default>
    	<locale name="C">
    	   <short>User's home timeline</short>
    	   <long>User's home timeline.</long>
    	</locale>
    </schema>
    
    <schema>
    	<key>/schemas/apps/twitux/tweets/reload_timeline</key>
    	<applyto>/apps/twitux/tweets/reload_timeline</applyto>
    	<owner>twitux</owner>
    	<type>bool</type>
    	<default>false</default>
    	<locale name="C">
    	   <short>Reload timeline</short>
    	   <long>Reload timeline.</long>
    	</locale>
    </schema>

    <schema>
	<key>/schemas/apps/twitux/ui/main_window_height</key>
	<applyto>/apps/twitux/ui/main_window_height</applyto>
	<owner>twitux</owner>
	<type>int</type>
	<default>0</default>
	<locale name="C">
	   <short>Main window height</short>
	   <long>Main window height.</long>
	</locale>
     </schema>

     <schema>
	<key>/schemas/apps/twitux/ui/main_window_width</key>
	<applyto>/apps/twitux/ui/main_window_width</applyto>
	<owner>twitux</owner>
	<type>int</type>
	<default>0</default>
	<locale name="C">
	   <short>Main window width</short>
	   <long>Main window width.</long>
	</locale>
     </schema>

     <schema>
	<key>/schemas/apps/twitux/ui/main_window_pos_x</key>
	<applyto>/apps/twitux/ui/main_window_pos_x</applyto>
	<owner>twitux</owner>
	<type>int</type>
	<default>0</default>
	<locale name="C">
	   <short>Main window X position</short>
	   <long>Main window X position.</long>
	</locale>
     </schema>

     <schema>
	<key>/schemas/apps/twitux/ui/main_window_pos_y</key>
	<applyto>/apps/twitux/ui/main_window_pos_y</applyto>
	<owner>twitux</owner>
	<type>int</type>
	<default>0</default>
	<locale name="C">
	   <short>Main window Y position</short>
	   <long>Main window Y position.</long>
	</locale>
     </schema>
     
     <schema>
	<key>/schemas/apps/twitux/ui/hide</key>
	<applyto>/apps/twitux/ui/hide</applyto>
	<owner>twitux</owner>
	<type>bool</type>
	<default>false</default>
	<locale name="C">
	   <short>Is the main window hidden</short>
	   <long>Is the main window hidden</long>
	</locale>
     </schema>
     
     <schema>
	<key>/schemas/apps/twitux/ui/expand_messages</key>
	<applyto>/apps/twitux/ui/expand_messages</applyto>
	<owner>twitux</owner>
	<type>bool</type>
	<default>false</default>
	<locale name="C">
	   <short>Expand messages</short>
	   <long>Expand messages</long>
	</locale>
     </schema>
     
     <schema>
	<key>/schemas/apps/twitux/ui/notify</key>
	<applyto>/apps/twitux/ui/notify</applyto>
	<owner>twitux</owner>
	<type>bool</type>
	<default>false</default>
	<locale name="C">
	   <short>Show notification of new Twitter messages</short>
	   <long>Show notification of new Twitter messages.</long>
	</locale>
     </schema>

     <schema>
      <key>/schemas/apps/twitux/ui/spell_checker_languages</key>
      <applyto>/apps/twitux/ui/spell_checker_languages</applyto>
      <owner>twitux</owner>
      <type>string</type>
      <default>en</default>
      <locale name="C">
	<short>Spell checking languages</short>
	<long>
	  Comma separated list of spell checker languages to use (e.g. en, fr, nl).
	</long>
      </locale>
     </schema>

     <schema>
	<key>/schemas/apps/twitux/ui/spell</key>
	<applyto>/apps/twitux/ui/spell</applyto>
	<owner>twitux</owner>
	<type>bool</type>
	<default>false</default>
	<locale name="C">
	   <short>Enable spell checking</short>
	   <long>Enable spell checking.</long>
	</locale>
     </schema>
     
     <schema>
	<key>/schemas/apps/twitux/ui/statusbar</key>
	<applyto>/apps/twitux/ui/statusbar</applyto>
	<owner>twitux</owner>
	<type>bool</type>
	<default>true</default>
	<locale name="C">
	   <short>Show the statusbar</short>
	   <long>Show the statusbar.</long>
	</locale>
     </schema>

  </schemalist>  
</gconfschemafile>
